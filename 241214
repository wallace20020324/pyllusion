  for (let j = 0; j < 8; j++) {
    let ellipseWidth = 60;  // 橢圓寬度
    let ellipseHeight = 160; // 橢圓高度
    let centerX = 1260 + i * distance;// X座標
    let centerY = 200 + j * spacing;// Y座標
    
    // 黑色半邊框
    strokeWeight(16);
    stroke(0); // 黑色
    arc(centerX, centerY, ellipseWidth, ellipseHeight, -HALF_PI, HALF_PI);

    // 白色半邊框
    stroke(255); // 白色
    arc(centerX, centerY, ellipseWidth, ellipseHeight, HALF_PI, -HALF_PI);

    // 填充橢圓
    noStroke();
    fill(R, G, B); // 填色
    ellipse(centerX, centerY, ellipseWidth, ellipseHeight);
  }
  
  for (let j = 0; j < 8; j++) {
    let ellipseWidth = 40;  // 橢圓寬度
    let ellipseHeight = 160; // 橢圓高度
    let centerX = 1340 + i * distance;// X座標
    let centerY = 200 + j * spacing;// Y座標
    
